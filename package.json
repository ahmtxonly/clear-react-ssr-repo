  "lint-staged": {
    "*.{js,jsx}": [
      "jest --bail --findRelatedTests",
      "eslint --no-ignore --fix"
    ],
    "*.{json,md}": [
      "prettier --write"
    ],
    "*.css": [
      "stylelint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "scripts": {
    "precommit": "lint-staged",
    "lint-js": "eslint .",
    "lint-css": "stylelint \"src/**/*.css\"",
    "lint": "yarn run lint-js && yarn run lint-css",
    "fix-js": "yarn run lint-js --fix",
    "fix-css": "yarn run lint-css --fix",
    "fix": "yarn run fix-js && yarn run fix-css",
    "test": "jest",
    "test-watch": "yarn run test --watch --notify",
    "test-cover": "yarn run test --coverage",
    "test-update-snapshot": "yarn run test --updateSnapshot",
    "coverage": "yarn run test-cover && open-cli coverage/lcov-report/index.html",
    "clean": "babel-node tools/run clean",
    "copy": "babel-node tools/run copy",
    "bundle": "babel-node tools/run bundle",
    "build": "babel-node tools/run build",
    "build-stats": "yarn run build --release --analyse",
    "render": "babel-node tools/run render",
    "serve": "babel-node tools/run runServer",
    "start": "babel-node tools/run start",
    "start:test": "export NODE_ENV=test && babel-node tools/run start"
  }
